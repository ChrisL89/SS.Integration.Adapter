<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!--
  <configSections>
    <section name="mappingUpdater" type="SS.Integration.Adapter.Configuration.MappingUpdaterSection, SS.Integration.Adapter" />
  </configSections>
  -->
  <appSettings>
    <add key="user"                               value='#{AdapterServiceUsername}' />
    <add key="password"                           value='#{AdapterServicePassword}' />
    <add key="url"                                value='#{AdapterServiceApiUrl}' />
    <add key="newFixtureCheckerFrequency" 			  value="#{AdapterNewFixtureCheckerFrequency}" />
    <add key="startingRetryDelay" 			          value="#{AdapterStartingRetryDelay}" />
    <add key="maxRetryDelay" 			                value="#{AdapterMaxRetryDelay}" />
    <add key="maxRetryAttempts" 			            value="#{AdapterMaxRetryAttempts}" />
    <add key="echoInterval" 			                value="#{AdapterEchoInterval}" />
    <add key="echoDelay" 			                    value="#{AdapterEchoDelay}" />
    <add key="fixtureCreationConcurrency" 			  value="#{AdapterFixtureCreationConcurrency}" />
    <add key="suspendAllOnShutdown" 			        value="#{AdapterSuspendAllOnShutdown}" />
    <add key="heartBeatIntervalSeconds" 			    value="#{AdapterHeartBeatIntervalSeconds}" />
    <add key="eventStateFilePath" 			          value="#{AdapterEventStateFilePath}" />
    <add key="marketFiltersDirectory" 			      value="#{AdapterMarketFiltersDirectory}" />
    <add key="cacheExpiryInMins" 			            value="#{AdapterCacheExpiryInMins}" />

    <!--<add key="newFixtureCheckerFrequency" value="60000" />
    <add key="startingRetryDelay" value="100" />
    <add key="maxRetryDelay" value="7000" />
    <add key="maxRetryAttempts" value="100" />
    <add key="echoInterval" value="10000" />
    <add key="echoDelay" value="3000" />
    <add key="fixtureCreationConcurrency" value="#{AdapterFixtureCreationConcurrency}" />
    <add key="suspendAllOnShutdown" value="true" />
    <add key="heartBeatIntervalSeconds" value="30" />
    <add key="eventStateFilePath" value="eventState.json" />
    <add key="marketFiltersDirectory" value="FixturesStateFiles\MarketFilterState" />
    <add key="cacheExpiryInMins" value="15" />-->
    
  </appSettings>
  <!--
  <mappingUpdater configSource="mappingUpdater.config" />
  -->
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.net>
    <connectionManagement>
      <!-- The RFC2616 spec recomends that a client should NOT maintain more than 2 connections C:\Git\Unified_Data_API_Client_DotNet\SportingSolutions.Udapi.Sdk.StreamingExample.Console\Udapi\
           with any server/proxy to improve response times and avoid congestion. This means highly concurrent, 
           latent web requests require excessive timeout values to avoid timeouts as a result of request queueing
           so the max connections value has been increased -->
      <add address="*" maxconnection="1000" />
    </connectionManagement>
  </system.net>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="RabbitMQ.Client" publicKeyToken="89e7d7c5feba84ce" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.1.5.0" newVersion="3.1.5.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>